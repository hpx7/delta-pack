{
  "name": "@hpx7/delta-pack-cli",
  "version": "0.2.0",
  "license": "MIT",
  "author": "hpx7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpx7/delta-pack.git",
    "directory": "cli"
  },
  "type": "module",
  "bin": {
    "delta-pack": "dist/index.js"
  },

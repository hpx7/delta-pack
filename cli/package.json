  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts src/codegen/index.ts --outdir dist --target node && tsc",
    "test": "bun test"
  },
  "dependencies": {
    "@hpx7/delta-pack": "^0.2.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
